/ {
  usb_phy0: phy0 {
    #phy-cells = <0>;
    compatible = "ulpi-phy";
    reg = <0xe0002000 0x1000>;
    view-port = <0x0170>;
    drv-vbus;
  };
  pps {
    compatible = "pps-gpio";
    gpios = <&gpio0 54 0>;
    capture-clear;
  };
};

&gem0 {
  phy-handle = <&ethernet_phy>;
  ethernet_phy: ethernet-phy@1 {
    reg = <1>;
  };
};

&i2c0 {
  eeprom@50 {
    compatible = "24c64";
    size = <8192>;
    pagesize = <32>;
    reg = <0x50>;
  };
};

&spi1 {
  spidev@0 {
    compatible = "ltc2488";
    reg = <0x0>;
    spi-max-frequency = <10000000>;
  };
};

&usb0 {
  dr_mode = "host";
  usb-phy = <&usb_phy0>;
  xlnx,phy-reset-gpio = <&gpio0 48 0>;
};

&adc {
  xlnx,channels {
    #address-cells = <1>;
    #size-cells = <0>;
    channel@0 {
      reg = <0>;
    };
    channel@1 {
      reg = <1>;
    };
    channel@2 {
      reg = <2>;
    };
    channel@9 {
      reg = <9>;
    };
    channel@10 {
      reg = <10>;
    };
  };
};
