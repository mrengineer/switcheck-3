/ {
	compatible = "xlnx,versal-vc-p-a2197-00-revA",
		     "xlnx,versal-vc-p-a2197-00",
		     "xlnx,versal-vc-p-a2197", "xlnx,versal";
	model = "Xilinx Versal A2197 Processor board revA";

	aliases {
		serial2 = &dcc;
	};
};

&dcc {
	status = "okay";
};

&sdhci0 {
	no-1-8-v;
};

&sdhci1 {
	no-1-8-v;
};
